package main
import "fmt"
type ll struct {
	a    int
	next struct *ll
}
func main() {
	var a1 ll
	var a2 ll
	var a3 ll
	var a4 ll
	a1.a = 1
	a2.a = 2
	a3.a = 3
	a4.a = 4
	a1.next = &a2
	a2.next = &a3
	a3.next = &a4
	a4.next = &a1
	var head *ll
	var foot *ll
	head = &a1
	foot= &a2
	found := 3
	for {
		if (*foot).a==3 {
			break
		}
		if (*head).a == 3 {
			found1:= 56
			break
		}
		bb:=45
	}

	if found==5 {
		yash:="too edgy"
	} else {
		yash:= "why so edgy?"
	}
}
