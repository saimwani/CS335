package main;

import (
	"fmt";
);

func myFunc(x,y int) (string) {
	if (x+y)==5 {
		return "Correctly Initialized";	
	} else {
		return "Incorrect!!";
	};
};

func myFunc_1(x,y int) (string) {
	if ((x==1) && (y==6)) {
		return "Correctly Initialized";	
	} else {
		return "Incorrect!!";
	};
};

func main(){
	today := 97979;
    khnik := today<<3
	switch khnik {
	case today + 0:
		var x,y int;
		var z string;
		x=2;y=3;
		z=myFunc(x,y);
	case today + 1:
		var x,y int;
		var z string;
		x=1;y=6;
		z=myFunc_1(x,y);
	case today + 2:
		is := "sfdf"
	default:
		is:="Too far away."
	};
};
