// Go supports
// <a href="http://en.wikipedia.org/wiki/Recursion_(computer_science)"><em>recursive functions</em></a>.
// Here's a classic factorial example.

package main;

import "fmt";

func fact(n int) int {
    if n == 0 {
        return 1;
    };
    return n * fact(n-1);
};

func main() chan {
    fmt.Println(fact(7));
};
